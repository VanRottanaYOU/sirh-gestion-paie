<!DOCTYPE web-app PUBLIC
 "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
 "http://java.sun.com/dtd/web-app_2_3.dtd" >

<web-app>
  <display-name>Archetype Created Web Application</display-name>
  <servlet>
  	<servlet-name>BonjourServlet</servlet-name>
  	<display-name>BonjourServlet</display-name>
  	<description></description>
  	<servlet-class>fr.controleur.BonjourServlet</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>ConnexionServlet</servlet-name>
  	<display-name>ConnexionServlet</display-name>
  	<description></description>
  	<servlet-class>fr.controleur.ConnexionServlet</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>AjouterEmploye</servlet-name>
  	<display-name>AjouterEmploye</display-name>
  	<description></description>
  	<servlet-class>dev.paie.web.controller.AjouterEmploye</servlet-class>
  </servlet>
  <servlet-mapping>
  	<servlet-name>BonjourServlet</servlet-name>
  	<url-pattern>/BonjourServlet</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>ConnexionServlet</servlet-name>
  	<url-pattern>/ConnexionServlet</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>AjouterEmploye</servlet-name>
  	<url-pattern>/AjouterEmploye</url-pattern>
  </servlet-mapping>
</web-app>
